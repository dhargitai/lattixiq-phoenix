{
  "name": "@phoenix/core",
  "version": "0.1.0",
  "private": true,
  "description": "Phoenix Framework Core Engine - Decoupled TypeScript logic",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@types/node": "^20.19.11",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}